<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>jsp02</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	
	
	<servlet>
		<servlet-name>AddManageTest</servlet-name>
		<servlet-class>controller.AddManageTest</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>AddManageTest</servlet-name>
		<url-pattern>/add</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>add</welcome-file>
	</welcome-file-list>
	
	
	<servlet>
		<servlet-name>EditManageTest</servlet-name>
		<servlet-class>controller.EditManageTest</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>EditManageTest</servlet-name>
		<url-pattern>/edit</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>edit</welcome-file>
	</welcome-file-list>
	
	
	<servlet>
		<servlet-name>DeleteManageTest</servlet-name>
		<servlet-class>controller.DeleteManageTest</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>DeleteManageTest</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>delete</welcome-file>
	</welcome-file-list>
	
	
	<servlet>
		<servlet-name>ListManageTest</servlet-name>
		<servlet-class>controller.ListManageTest</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>ListManageTest</servlet-name>
		<url-pattern>/list</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>list</welcome-file>
	</welcome-file-list>


</web-app>